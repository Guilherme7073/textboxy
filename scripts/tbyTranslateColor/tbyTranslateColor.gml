///@function tbyTranslateColor(inputNum)
///@desc Takes a number and translates it according to the color enum
///@param inputNum

switch (argument0) {
	case 1: return TbyColor.Red; break;
	case 2: return TbyColor.Orange; break;
	case 3: return TbyColor.Yellow; break;
	case 4: return TbyColor.Green; break;
	case 5: return TbyColor.Blue; break;
	case 6: return TbyColor.Purple; break;
	default: return TbyColor.White;
}
